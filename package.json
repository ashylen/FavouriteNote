{
  "name": "favnote",
  "version": "0.1.0",
  "homepage": "https://ashylen.github.io/FavouriteNote/",
  "private": false,
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "gh-pages": "^2.1.1",
    "react": "^16.9.0",
    "react-cookie": "^4.0.1",
